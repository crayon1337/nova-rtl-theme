{
    "name": "crayon/nova-rtl-theme",
    "description": "RTL layout for Laravel Nova. This package has been forked from pishran/nova-rtl-theme",
    "keywords": [
        "laravel",
        "nova",
        "rtl",
        "theme"
    ],
    "homepage": "https://github.com/crayon/nova-rtl-theme",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Mahmoud",
            "email": "mohammedarey2@gmail.com",
            "homepage": "https://stack-flare.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Pishran\\NovaRtlTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pishran\\NovaRtlTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
